  <!DOCTYPE html>
  <html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Technograv{% endblock %}</title>

    {% block stylesheets %}
      <link rel="stylesheet" href="{{ asset('bundles/tgadmin/bootstrap/css/bootstrap.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tgadmin/bootstrap/css/bootstrap-theme.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tgadmin/ckeditor/contents.css') }}" />

    {% endblock %}
  </head>

  <body>
    {% include "::nav-bar.html.twig" %}
        <div class="container">
      <div id="header" class="jumbotron">
      <h1>Technograv</h1>
        <p>
          Panneau d'administration
        </p>
      </div>

      <div class="row">
        <div id="menu" class="col-md-3">
          <h3>Paramètres</h3>
          <ul class="nav nav-pills nav-stacked">
            <li><a href="{{ path('tg_admin_types') }}">Types de projet</a></li>
            <li><a href="{{ path('tg_admin_etapes') }}">Etapes de projet</a></li>
          </ul>

          <h3>Utilisateurs</h3>
          <ul class="nav nav-pills nav-stacked">
            <li><a href="{{ path('tg_admin_comptes') }}">Comptes</a></li>
            <li><a href="{{ path('tg_admin_droits') }}">Droits</a></li>
          </ul>

        </div>
        <div id="content" class="col-md-9">
          {% block body %}
          {% endblock %}
        </div>
      </div>
    </div>
  </div>

      <hr>

      <footer align="center">
        <p>The sky's the limit © {{ 'now'|date('Y') }} and beyond.</p>
      </footer>
    </div>

    {% block javascripts %}
      <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <script src="{{ asset('bundles/tgadmin/bootstrap/js/bootstrap.min.js') }}"></script>
  <script src="{{ asset('bundles/tgadmin/ckeditor/ckeditor.js') }}"></script>
    {% endblock %}

  </body>

  </html>
